.head-view{
  width: 150rpx;
  height: 150rpx;
  border-radius: 75rpx;
}
.item-content-view{
  width: 100%;
  display: flex;
  flex-direction: column;

}
.item-view{
   font-size: 30rpx;
   color: gray;
   height: 100rpx;
   display: flex;
   justify-content: start;
   align-items: center;
   margin: 20rpx;
   border-bottom: 1rpx dashed rgb(180, 149, 103);
}
.left-image{
  width: 70rpx;
  height: 70rpx;
  margin-right: 20rpx;
}
.submit-view{
  margin-top: 90rpx;
  color: white;
  background-color: rgb(219, 183, 115);
}
.switch-view{
   display: flex;
   flex-direction: row;
   align-items: center;
   justify-content: flex-end;
   font-size: 30rpx;
   color: rgb(163, 164, 250);
   margin: 20rpx;
}
.switch{
  margin-left: 44rpx;
}