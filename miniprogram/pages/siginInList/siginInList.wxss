.container{
 
  width: 100%;
  height: 100%;
  background-color: rgb(246, 246, 246);
  overflow-x: hidden;
  
}
.item-view{
  width: 100%;
  display: flex;
  align-items: center;
  padding-left: 44rpx;
  margin-bottom: 22rpx;
  
}
.name-view{
  font-size: 30rpx;
  color: black;
  width: 120rpx;

}
.avatar-view{
  width: 100rpx;
  height: 100rpx;
  margin: 20rpx;
  border-radius: 12rpx;
  background-color: rgb(246, 246, 246);

}
.time-view{
  font-size: 28rpx;
  color: gray;
  margin-right: 20rpx;
  width: 65rpx;
}
.right-view{
  display: flex;
  align-items: center;
  background-color: white;
  flex: 1;
  position: relative;
}
.tianjia{
  font-size: 60rpx;
  right: 44rpx;
  bottom: 44rpx;
  position: fixed;
  color:rgb(180, 149, 103);
}

.schedule-view{
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.no-schedule-view{
   font-size: 28rpx;
   color:rgb(189, 189, 236);
   text-align: center;
   display: flex;
   align-items: center;

}
.signin-text{
   position: absolute;
   left: 75rpx;
   width: 120rpx;
   height: 120rpx;
   border-radius: 50%;
   font-size: 22rpx;
   text-align: center;
   line-height: 120rpx;
   
}
.unsigin{
  border:1px solid orange;
  color: orange;
}
.sigin{
  border:0.8px dotted lightgray;
  color: lightgray;
}
.signin-view{
  position: absolute;
  right: 50rpx;
  font-size: 65rpx;
  
}
.unsigin2{
  color: orange;
}
.sigin2{
  color: lightgray;
}