<view class="container">
    <block wx:for="{{customers}}" wx:key="index">
       <view class="item-view">
          <text class="time-view">{{daySchedule[index].worktime_begin}}</text>
          <text class="time-view">{{daySchedule[index].worktime_end}}</text>
          <view class="right-view">
            <image src="{{item.avatar}}" mode="aspectFill" class="avatar-view"></image>
            <text class="name-view">{{item.name}}</text>
          </view>
          
       </view>
      </block>
</view>

<text class="iconfont icon-tianjia tianjia" bindtap="addSchedule"></text>
